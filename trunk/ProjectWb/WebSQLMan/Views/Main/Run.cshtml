@model System.Data.DataSet
@using System.Data;


@foreach (DataTable dt in Model.Tables)
{
    

    @(
    
    Html.X().Panel()
    .Title(dt.TableName)
    .AutoRender(true)
    .Closable(true)
    .Region(Region.Center)
    .BodyPadding(10)
    .AutoScroll(true)
    .Items( Html.X().GridPanelFor<DataTable>(dt,true,true,true)
                .Title(dt.TableName)
                .AutoScroll(true)
                .LayoutConfig(new HBoxLayoutConfig { Align = HBoxAlign.StretchMax })
                .ForceFit(true)
                    )
         )
    
}   

